{
  "name": "protractor-smartrunner",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "webdriver-manager update --gecko=false --versions.chrome=$npm_package_config_chromeDriver",
    "ng": "ng",
    "start": "ng serve",
    "ci": "./ci.sh",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "pree2e": "npm run build:plugin",
    "e2e:exclusion": "protractor ./e2e/case-with-exclusion/protractor.conf.ts",
    "e2e:empty-exclusion": "protractor ./e2e/case-with-empty-exclusion/protractor.conf.ts",
    "e2e": "export GIT_HASH=`git rev-parse HEAD` && npm run e2e:exclusion && npm run e2e:empty-exclusion",
    "build:plugin": "tsc --project ./src/smartrunner"
  },
  "config": {
    "chromeDriver": "78.0.3904.70"
  },
  "dependencies": {
    "filenamify": "^4.1.0",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@angular/animations": "~8.2.8",
    "@angular/common": "~8.2.8",
    "@angular/compiler": "~8.2.8",
    "@angular/core": "~8.2.8",
    "@angular/forms": "~8.2.8",
    "@angular/platform-browser": "~8.2.8",
    "@angular/platform-browser-dynamic": "~8.2.8",
    "@angular/router": "~8.2.8",
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "~8.3.6",
    "@angular/compiler-cli": "~8.2.8",
    "@angular/language-service": "~8.2.8",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "codelyzer": "^5.0.0",
    "jasmine": "~3.4.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.9.0",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lite-server": "^2.5.4",
    "protractor": "~5.4.0",
    "protractor-retry": "^1.2.9",
    "puppeteer": "1.20.0",
    "ts-jest": "^24.1.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "3.5.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  }
}
